{
  "App": {
    "name": "identity-service"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "SeqOptions": {
    "Enabled": true,
    "ApiKey": null,
    "SeqServerUrl": "http://localhost:5340/"
  },
  "Logstash": {
    "Enabled": false,
    "LogstashgUrl": null
  },
  "MedPark360IdentityStore": {
    "ConnectionString": "Server=LOCALHOST;Database=medpark-identity;Trusted_Connection=True;MultipleActiveResultsets=true;ConnectRetryCount=5;ConnectRetryInterval=10;Connect Timeout=30;"
  },
  "restEase": {
    "loadBalancer": "",
    "services": [
      {
        "name": "med-practice-service",
        "scheme": "http",
        "host": "localhost",
        "port": "5002"
      }
    ]
  },
  "Consul": {
    "Enabled": true,
    "Host": "http://localhost:8500",
    "service": "identity-service",
    "address": "localhost",
    "Port": 7000,
    "PingEnabled": false,
    "removeAfterInterval": 10,
    "requestRetries": 3,
    "services": [
    ]
  }
